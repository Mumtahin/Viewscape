@{
    ViewBag.Title = "Contact";
}

<div class="my-container">
    <div class="container">
        <div class="well">
            <h1 class="database-header text-center animated fadeIn animated fadeIn">Contact Me</h1>
            <div class="contact-panel text-center animated fadeInUp">
                <h3>@ViewBag.Message</h3>

                <address>
                    <span class="glyphicons glyphicons-user"></span>C# and Asp.Net Developer<br />
                    Based in Shoreditch, London<br />
                    @*<abbr title="Phone">P:</abbr>
                        425.555.0100*@
                </address>

                <div class="vertical-rhythm">
                    <address>
                        <strong>Support: </strong><span id="email-link"></span><br />
                        <strong>Contact: </strong><span id="contact-number"></span>
                    </address>
                    <br />
                    <br />
                </div>
            </div>
        </div>
    </div>
</div>
